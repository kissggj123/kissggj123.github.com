{
   "author": "泡芙老司机",
   "background": {
      "persistent": false,
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "image/icon128.png",
         "64": "image/icon64.png"
      },
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "css": [ "css/style.css" ],
      "js": [ "js/content.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "homepage_url": "https://www.timefly.cn/free-auto-refresh",
   "icons": {
      "128": "image/icon128.png",
      "64": "image/icon64.png"
   },
   "manifest_version": 2,
   "name": "网页自动刷新 修改版crx3",
   "options_page": "options.html",
   "permissions": [ "activeTab", "storage", "browsingData", "notifications" ],
   "short_name": "Auto Refresh 2.3.3",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.3.3"
}
